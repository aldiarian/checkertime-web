<?xml version='1.0' encoding='utf-8'?>
<widget id="net.elojografico.checkertime" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>CheckerTime</name>
    <description>
        La aplicación para fichar en el entorno de control de presencia de checkertime.com.
    </description>
    <author email="info@elojografico.net" href="http://elojografico.net">
        El Ojo Gráfico
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="true" />
</widget>
